package com.day2;

public class SquareRootOfX {
	
	public static int firstOccuranceOfRepeatedIntegerFromRight(int[] arr, int x){
		int left = 0;
		int right = arr.length - 1;
		int ans = -1;
		while(left <= right){
			int mid = (left + right)/2;
			if(arr[mid] == x){
				ans = mid;
				left = mid +1;
			}
			else if(arr[mid] > x)
				right = mid - 1;
			else 
				left = mid + 1;
		}
		return ans;
	}

	public static void main(String[] args) {
		int x = 5;
		int index = squareRoot( x);

	}

}
