	static void push2(int x){
		q1.add(x);
	}
	
	static int pop2(){
		if(q1.isEmpty())
			return -1;
		while(q1.size() > 0){
			
		}
		
		return q1.poll();
	}
	
	static void swapQueues(Queue<Integer> x, Queue<Integer> y){
		q1 = y;
		q2 = x;
	}
	
	public static void main(String[] args) {
		int arr [] = {1,2,3,4};
		
		for(int i = 0; i < arr.length; i++){
			push2(arr[i]);
		}
		
		for(int i = 0; i < q1.size(); i++)
			System.out.println(pop2());
		
	}
}
