	static int poll2(){
		return s1.pop();
	}
	static void add2(int x){
		while(!s1.isEmpty()){
			s2.add(s1.pop());
		}
		s1.add(x);
		while(!s2.isEmpty()){
			s1.add(s2.pop());
		}
		int ret = s1.pop();
		
	}
	
	public static void main(String[] args){
		int arr[] = {1,2,3,4};
		
		for(int i = 0; i < arr.length; i++){
			add2(arr[i]);
		}
		
		for(int i = 0; i < 4; i++)
			System.out.println(poll2());

	}

}
