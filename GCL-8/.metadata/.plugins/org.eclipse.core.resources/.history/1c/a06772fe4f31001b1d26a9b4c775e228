package Contest2;

import java.util.ArrayList;
import java.util.Arrays;




public class XOR
{
		public static int convertFive(int n){
		    int temp = 0;
		    int i = 0;
	        while(n > 0){
	            int digit = n % 10;
	            if(digit == 5){
	                digit = 0;
	            }
	            temp  = (int) (temp + Math.pow(10, i) * digit);
	            n = n/10;
	            i++;
	        }
	        return temp;
	    }
	
	public static void main(String[] args){
		//int[] arr = {1, 3, 4, 1, 3, 8};
		System.out.println(convertFive(1554));
	}
