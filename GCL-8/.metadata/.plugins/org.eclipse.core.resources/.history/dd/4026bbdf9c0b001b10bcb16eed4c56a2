package com.day7;

import java.util.Stack;

public class InfixToPostfix {
	Stack st = new Stack();
	public static void postfix(String str){
		int n = str.length();
		int i = 0;
		while(i < n)
		System.out.println(str);
	}
	public static void main(String[] args) {
		String str = "(a+(b*d)+d)";
		postfix(str);
	}

}
